{
    "name": "kioskemploi/txtextcontrol-reportingcloud",
    "description": "PHP wrapper for ReportingCloud Web API. Authored and supported by Text Control GmbH.",
    "license": "BSD-3-Clause",
    "type": "library",
    "autoload": {
        "psr-4": {
            "TxTextControl\\ReportingCloud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TxTextControlTest\\ReportingCloud\\": "test/"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "zf1s/zend-filter": "^2.7",
        "zf1s/zend-servicemanager": "^3.2",
        "zf1s/zend-validator": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^3.0",
        "smalot/pdfparser": "^0.9"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/phpcs src test --standard=phpcs.xml"
    }
}
